prefix=D:/Aplicativos/GNOME
libdir=${prefix}/lib
includedir=${prefix}/include

Name: cairo-script-interpreter
Description: script surface backend for cairo graphics library
Version: 1.17.5
Requires: cairo
Requires.private: zlib, fontconfig, freetype2, expat, pixman-1
Libs: -L${libdir} -lcairo-script-interpreter
Libs.private: -lgdi32 -lmsimg32 -L${libdir} -lpng16
Cflags: -I${includedir}/cairo
